@software{rajeg_supplementary_verb_affix_2023,
  title = {Material Pendukung Untuk {{{\mkbibemph{Afiksasi Verba}}}}{\mkbibemph{ Dalam }}{{{\mkbibemph{Bahasa Indonesia}}}}},
  author = {Rajeg, Gede Primahadi Wijaya and Denistia, Karlina},
  date = {2023-04-10},
  doi = {10.5281/zenodo.7812619},
  url = {https://doi.org/10.5281/zenodo.7812619},
  urldate = {2023-05-27},
  abstract = {Catatan pemutakhiran pada versi 0.0.3 Perbaikan anotasi morfologis untuk diperhatikan dan diperbaiki Perbaikan anotasi morfologis untuk memasak Penambahan kode analisis T-Test untuk rerata frekuensi token dan tipe di- vs. di-/-kan pada analisis produktivitas berdasarkan tahun Menghasilkan kembali semua grafik berkaitan dengan perbaikan di atas ========================= Deskripsi Repositori ini membagikan bahan pendukung (data, grafik statistik, dan kode pemrograman R untuk analisis statistik dan visualisasi) dari makalah yang berjudul Afiksasi verba dalam bahasa Indonesia. Makalah ini menjadi bagian dari proyek buku Tatabahasa Bahasa Indonesia Kontemporer (TBIK) berbasis korpus (flyer seminar hasil). Bahan pendukung dari makalah kami yang lain untuk proyek buku yang sama dapat diakses melalui tautan berikut. Saksikan rekaman presentasi seminar hasil makalah kami pada tautan ini (Afiksasi verba dalam bahasa Indonesia) dan ini (Afiksasi nomina dalam bahasa Indonesia). Berikut ini adalah cara mengutip repositori ini (dengan selingkung APA) jika menggunakan material di dalamnya: Rajeg, G. P. W., \& Denistia, K. (2023). Material pendukung untuk 'Afiksasi Verba dalam Bahasa Indonesia' (Version 0.0.3) [Computer software]. https://doi.org/10.17605/OSF.IO/NUXD4},
  organization = {Zenodo},
  version = {0.0.3}
}

